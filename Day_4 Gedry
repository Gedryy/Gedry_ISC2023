package new4;

public class Main{
				
				public static void main(String[] args) {
        String nama;
        char inisial;
        int umur;
        float berat;
        double tinggi;
        boolean belajar;
        
        //pemberian nilai ke variabel
        nama = "GEDRY";
        inisial = 'G';
        umur = 18;
        berat = 50.5f;
        tinggi = 165.5;
        belajar = true;
        
        //Menampilkan hasil
        System.out.println("Namanya \t\t: " + nama );
        System.out.println("Berinisial \t\t : " + inisial);
        System.out.println("Umurnya \t\t : " + umur);
        System.out.println("Beratny \t\t : " + berat);
        System.out.println("Tinggiku \t\t :" + tinggi);
        System.out.println("Laper sih \t\t:" + belajar );
        
    
    
				    
    }
    
}
