package Java;
import java.util.*;
public class Main {
    
    public static void main(String[] args) {
        Scanner rahel = new Scanner(System.in);
        
      
        
        String [] angka = new String[5];
        String c = null;
        for(int i = 0; i < angka.length; i++){
            
            System.out.print(" Masukan Nama :");
            c = rahel.nextLine();
            angka[i] = c;
    }
        System.out.println(Arrays.toString(angka));       
        
    
}

}
