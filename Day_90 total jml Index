package Java;
import java.util.*;
public class Main {
    
    public static void main(String[] args) {
        Scanner rahel = new Scanner(System.in);
        
     
        int [] a = {1,2,3};
        int total = 0;
        int jml_index = a.length;
        
       
        for(int i = 0; i < jml_index; i++){
            total += a[i];
        }
        System.out.println(total);
        
        
        
    }

}
