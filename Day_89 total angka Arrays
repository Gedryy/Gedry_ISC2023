package Java;
import java.util.*;
public class Main {
    
    public static void main(String[] args) {
        Scanner rahel = new Scanner(System.in);
        
      
        
        int [] angka = new int[5];
        int c = 0;
        for(int i = 0; i < angka.length; i++){
            
            System.out.print("Masukan angka :");
            c = rahel.nextInt();
            angka[i] = c;
    }
        System.out.println(Arrays.toString(angka));       
        
    
}

}
